{
    "llvm-target": "thumbv7em-none-eabi",
    "linker": "arm-none-eabi-gcc",
    "target-endian": "little",
    "target-pointer-width": "32",
    "arch": "arm",
    "os": "none",
    "cpu": "cortex-m4",
    "features": "+thumb2",
    "relocation-model": "static",
    "linker-is-gnu": true,
    "has-rpath": true,
    "morestack": false,
    "disable-redzone": true,
    "executables": true,
    "dynamic_linking": false,
    "no-compiler-rt": true
}
